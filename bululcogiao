	local StatusGame = Window:MakeTab({
		Name = "Status",
		Icon = "rbxassetid://",
		PremiumOnly = false
		})
		
		local StatusGameSection = StatusGame:AddSection({
		Name = "Status"
		})

		local WeaponCheck = StatusGameSection:AddLabel("Weapon Select : ")

		spawn(function()
			pcall(function()
				while wait() do
					WeaponCheck:Set("Weapon Select :  ".._G.Select_Weapon )
				end
			end)
		end)

		local BoneCheck = StatusGameSection:AddLabel("Bone Check : 0")

		spawn(function()
			pcall(function()
				while wait() do
					BoneCheck:Set("Bone Check : "..(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones","Check")))
				end
			end)
		end)

		local EctoplasmCheck = StatusGameSection:AddLabel("Ectoplasm Check : 0")

		spawn(function()
			pcall(function()
				while wait() do
					EctoplasmCheck:Set("Ectoplasm Check : "..(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Ectoplasm","Check")))
				end
			end)
		end)

		local KatakuriCheck = StatusGameSection:AddLabel("Katakuri Check : ❔️")

		spawn(function()
			while wait() do
				pcall(function()
					if string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 88 then
						KatakuriCheck:Set("Katakuri Check : Need Kill "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,41))
					elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 87 then
						KatakuriCheck:Set("Katakuri Check : Need Kill "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,40))
					elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 86 then
						KatakuriCheck:Set("Katakuri Check : Need Kill "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,39))
					else
						KatakuriCheck:Set("Katakuri Check : 🟢")
					end
				end)
			end
		end)

		local EliteCheck = StatusGameSection:AddLabel("Elite Check : ❔️")

        spawn(function()
            while wait() do
                pcall(function()
                    if game:GetService("ReplicatedStorage"):FindFirstChild("Diablo [Lv. 1750]") then
                        EliteCheck:Set("Elite Check : [ Diablo ] Lv.1750")	
                    elseif game:GetService("ReplicatedStorage"):FindFirstChild("Deandre [Lv. 1750]") then
					    EliteCheck:Set("Elite Check : [ Deandre ] Lv.1750")	
					elseif game:GetService("ReplicatedStorage"):FindFirstChild("Urban [Lv. 1750]") then
						EliteCheck:Set("Elite Check : [ Urban ] Lv.1750")	
					else
                        EliteCheck:Set("Elite Check : 🔴")	
                    end
                end)
            end
        end)

		local MysticCheck = StatusGameSection:AddLabel("Mystic Check : ❔️")

		task.spawn(function()
			while task.wait() do
				pcall(function()
					if game.Workspace._WorldOrigin.Locations:FindFirstChild("Mirage Island") then
						MysticCheck:Set("Mystic Island Check : 🟢")
					else
						MysticCheck:Set("Mystic Island Check : 🔴")
					end
				end)
			end
		end)

		local FullMoonCheck = StatusGameSection:AddLabel("Full Moon Check: 🔴")

		task.spawn(function()
			while task.wait() do
				pcall(function()
					if game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149431" then
						FullMoonCheck:Set("Full Moon Check : 100%")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149052" then
						FullMoonCheck:Set("Full Moon Check : 75%")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709143733" then
						FullMoonCheck:Set("Full Moon Check : 50%")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709150401" then
						FullMoonCheck:Set("Full Moon Check : 25%")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149680" then
						FullMoonCheck:Set("Full Moon Check : 15%")
					else
						FullMoonCheck:Set("Full Moon Check : 0%")
					end
				end)
			end
		end)

		local Teleport = Window:MakeTab({
