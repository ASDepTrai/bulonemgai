	local Settings = Window:MakeTab({
		Name = "Settings",
		Icon = "rbxassetid://",
		PremiumOnly = false
		})
		
		local SettingsSection = Settings:AddSection({
		Name = "Settings Section"
		})
	
	SettingsSection:AddToggle({
		Name = "Fast Attack",
		Default = _G.Settings.Clickattack,
		Callback = function(va)
			getgenv().Clickattack = va
			_G.Settings.Clickattack = va
			saveSettings()
		end    
	})
	
